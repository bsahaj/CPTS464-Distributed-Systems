SOURCE_DIR = 

TARGET_ARCH=x64Darwin17clang9.0

# If undefined in the environment default NDDSHOME to install dir
ifndef NDDSHOME
NDDSHOME := /Applications/rti_connext_dds-6.0.1
endif

JAVA_PATH = java
JAVAC_PATH = javac

JAVA_SOURCES = $(SOURCE_DIR)./Position.java $(SOURCE_DIR)./PositionSeq.java $(SOURCE_DIR)./PositionTypeSupport.java $(SOURCE_DIR)./PositionTypeCode.java $(SOURCE_DIR)./PositionDataReader.java $(SOURCE_DIR)./PositionDataWriter.java $(SOURCE_DIR)./Accident.java $(SOURCE_DIR)./AccidentSeq.java $(SOURCE_DIR)./AccidentTypeSupport.java $(SOURCE_DIR)./AccidentTypeCode.java $(SOURCE_DIR)./AccidentDataReader.java $(SOURCE_DIR)./AccidentDataWriter.java $(SOURCE_DIR)./AccidentSubscriber.java $(SOURCE_DIR)./AccidentPublisher.java $(SOURCE_DIR)./PassengerSubscriber.java #(SOURCE_DIR)./OperatorSubscriber.java
CLASS_FILES  = $(JAVA_SOURCES:%.java=%.class)

RTI_CLASSPATH := $(NDDSHOME)/lib/java/nddsjava.jar

%.class : %.java
	$(JAVAC_PATH)  -classpath ".:$(RTI_CLASSPATH)" $<

all: $(CLASS_FILES)
 
#
# Convenient way to run the java programs
#

export DYLD_LIBRARY_PATH := $(NDDSHOME)/lib/x64Darwin17clang9.0:/usr/lib/lwp:$(DYLD_LIBRARY_PATH)

AccidentPublisher: ./AccidentPublisher.class
	$(JAVA_PATH) -Djava.library.path=$(NDDSHOME)/lib/x64Darwin17clang9.0 -classpath ".:$(RTI_CLASSPATH)" AccidentPublisher $(ARGS)
 
AccidentSubscriber: ./AccidentSubscriber.class
	$(JAVA_PATH) -Djava.library.path=$(NDDSHOME)/lib/x64Darwin17clang9.0 -classpath ".:$(RTI_CLASSPATH)" AccidentSubscriber $(ARGS)
PassengerSubscriber: ./PassengerSubscriber.class
	$(JAVA_PATH) -Djava.library.path=$(NDDSHOME)/lib/x64Darwin17clang9.0 -classpath ".:$(RTI_CLASSPATH)" PassengerSubscriber $(ARGS)
OperatorSubscriber: ./OperatorSubscriber.class
	$(JAVA_PATH) -Djava.library.path=$(NDDSHOME)/lib/x64Darwin17clang9.0 -classpath ".:$(RTI_CLASSPATH)" OperatorSubscriber $(ARGS)
PassengerSubscriber1: ./PassengerSubscriber
	java -Djava.library.path=/Applications/rti_connext_dds-6.0.1/lib/x64Darwin17clang9.0 -classpath ".:/Applications/rti_connext_dds-6.0.1/lib/java/nddsjava.jar" PassengerSubcriber Express1 1 4 | grep -i -v 'rti'
PassengerSubscriber2: ./PassengerSubscriber
	java -Djava.library.path=/Applications/rti_connext_dds-6.0.1/lib/x64Darwin17clang9.0 -classpath ".:/Applications/rti_connext_dds-6.0.1/lib/java/nddsjava.jar" PassengerSubcriber Express2 3 2 | grep -i -v 'rti'